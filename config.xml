<widget versionCode="2" version="2.6" id="com.devexpress.apptemplate" xmlns:gap="http://phonegap.com/ns/1.0" xmlns="http://www.w3.org/ns/widgets">
  <name>mySAISD</name>
  <preference value="none" name="permissions" />
  <preference value="true" name="prerendered-icon" />
  <preference value="adjustPan" name="android-windowSoftInputMode" />
  <preference value="splash" name="SplashScreen" />
  <preference value="10000" name="SplashScreenDelay" />
  <preference value="false" name="AutoHideSplashScreen" />
  <preference value="true" name="DisallowOverscroll" />
  <preference value="false" name="StatusBarOverlaysWebView" />
  <preference value="#000000" name="StatusBarBackgroundColor" />
  <preference value="14" name="android-minSdkVersion" /> 
  <preference value="26" name="android-targetSdkVersion" />
  <feature name="http://api.phonegap.com/1.0/network" />  
  <gap:plugin version="1.1.0" name="cordova-plugin-ios-longpress-fix" source="npm" />
  <preference name="phonegap-version" value="cli-6.4.0" />
  <gap:plugin onload="true" version="2.1.0" name="cordova-plugin-statusbar" source="npm" />
  <gap:plugin name="cordova-plugin-whitelist" source="npm" />
  <access origin="*" />
  <preference name="FadeSplashScreen" value="false" />
  <preference name="ShowSplashScreenSpinner" value="false" />
  <key>CFBundleVersion</key>
  <string>2.6.0</string>
  <key>CFBundleShortVersionString</key>
  <string>2.6</string>
  <icon platform="ios" src="Resources/mySAISD-splashscreen1024x1024.png" width="1024" height="1024" />
  <!--<icon platform="ios" src="Resources/mySAISD-splashscreen120x120.png" width="120" height="120" />
  <icon platform="ios" src="Resources/mySAISD-splashscreen152x152.png" width="152" height="152" />
  <icon platform="ios" src="Resources/mySAISD-splashscreen76x76.png" width="76" height="76" />-->
</widget>